    public static String makeServiceTeam(String code,String userID, String annotID, boolean innerRole ) {

    	String exOpen = "<SERVICETEAM>";
    	String exClose = "</SERVICETEAM>";
    	
    	String CodeOpen = "<SERVICEROLECODE>";
    	String CodeClose = "</SERVICEROLECODE>";   	
    	String UserIDOpen = "<USERID>";
    	String UserIDClose = "</USERID>";
    	String AonnetIDOpen = "<AONNETID>";
    	String AonnetIDClose = "</AONNETID>";
    	
    	//| annotID.length() == 0 
    	
    	if ( annotID == null ) {
    		return "";
    	} else {
    		if (innerRole) {
    			return CodeOpen + code + CodeClose + UserIDOpen + userID + UserIDClose + AonnetIDOpen + annotID + AonnetIDClose + exClose + exOpen;
    		} else {
    			return exOpen + CodeOpen + code + CodeClose + UserIDOpen + userID + UserIDClose + AonnetIDOpen + annotID + AonnetIDClose + exClose;
    		}
    	}
    	
    }  
    
    
    

    
    
